<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.data.dto">
<class name="Survey" table="SURVEY" dynamic-update="false" dynamic-insert="false">
<cache usage="read-write"></cache>
<id name="id" column="ID" type="java.lang.Long">
<generator class="increment"></generator>
</id>
<property name="name" type="java.lang.String" update="true" insert="true" access="property" column="name"></property>

</class>
</hibernate-mapping>